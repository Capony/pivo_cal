<div class="wrapper">
<div class="content">
<h1 class="headline">Project</h1>
<p id="notice"><%= notice %></p>

<div class="project_box">
<p>
  <span style="color:black">Name:</span>
  <b><%= @project.name %></b>
</p>

<p>
  <span style="color:black">Tracker:</span>
  <b><%= @project.tracker_id %></b>
</p>

<p>
  <span style="color:black">Pivotal-Project ID:</span>
  <b><%= @project.project_id %></b>
</p>

</div>


<%= link_to 'Edit', edit_tracker_project_path(params[:tracker_id], @project.id) %> |
<%= link_to 'Back', tracker_projects_path(params[:tracker_id]) %>
</div>
</div>